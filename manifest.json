{
    "manifest_version": 3,
    "name": "Site Blocker",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "pageCapture",
        "webRequest",
        "storage"
    ],
    "options_page": "./options.html",
    "background": {
        "service_worker": "./background.js"
    }
}